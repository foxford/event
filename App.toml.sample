id = "event.dev.svc.example.org"
agent_label = "alpha"
broker_id = "mqtt-gateway.dev.svc.example.org"
backend_url = "http://0.0.0.0:8000"

[id_token]
algorithm = "ES256"
key = "data/keys/svc.private_key.p8.der.sample"

[authn."iam.internal"]
audience = ["internal"]
algorithm = "HS256"
key = "docker/backend/conf.d/org.usr.example.iam.key"

[authn."iam.dev.usr.example.org"]
audience = ["dev.usr.example.org"]
algorithm = "HS256"
key = "docker/backend/conf.d/org.usr.example.iam.key"

[authz."example.net"]
type = "http"
uri = "https://iam.example.net/authz"
algorithm = "ES256"
key = "data/keys/svc.private_key.p8.der.sample"

[mqtt]
uri = "mqtt://0.0.0.0:1883"
clean_session = false
